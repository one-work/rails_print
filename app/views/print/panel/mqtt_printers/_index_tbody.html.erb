<td class="family-monospace">
  <p><%= model.dev_imei %></p>
  <p><time data-controller="time"><%= model.created_at.to_fs(:created_at) %></time></p>
</td>
<td><%= model.dev_desc %></td>
<td><%= model.online %></td>
<td>
  <p><%= model.organ.name if model.organ %></p>
  <p>
    <%= button_to({ action: 'organ', id: model.id }) do %>
      <%= svg_tag 'pencil' %>
    <% end %>
  </p>
</td>
<td>
  <p><%= model.username %></p>
  <p><%= model.password %></p>
</td>